{
    "name": "primarybid-url-shortener",
    "version": "0.0.1",
    "description": "Simple full-stack application that provides URL shortening functionality",
    "main": "./dist/server/index.js",
    "scripts": {
        "start": "node ./dist/server/index.js",
        "dev": "concurrently \"npm run client:watch\" \"npm run server:watch\"",
        "client:build": "cross-env NODE_ENV=production rollup -c",
        "client:watch": "cross-env NODE_ENV=development rollup -cw",
        "server:build": "tsc --project ./tsconfig.server.json",
        "server:watch": "nodemon",
        "test": "npm run test:client && npm run test:server",
        "test:client": "jest --selectProjects client",
        "test:server": "jest --selectProjects server --coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/samuellaycock/primarybid-url-shortener.git"
    },
    "author": {
        "name": "Samuel Laycock",
        "email": "samuel.paul.laycock@gmail.com"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/samuellaycock/primarybid-url-shortener/issues"
    },
    "homepage": "https://github.com/samuellaycock/primarybid-url-shortener#readme",
    "dependencies": {
        "body-parser": "1.19.0",
        "cross-env": "7.0.2",
        "debug": "4.1.1",
        "dotenv": "8.2.0",
        "ejs": "3.1.5",
        "express": "4.17.1",
        "mongoose": "5.10.0",
        "randomstring": "1.1.5",
        "vue": "2.6.11"
    },
    "devDependencies": {
        "@babel/core": "7.11.1",
        "@babel/preset-env": "7.11.0",
        "@babel/preset-typescript": "7.10.4",
        "@rollup/plugin-alias": "3.1.1",
        "@rollup/plugin-commonjs": "15.0.0",
        "@rollup/plugin-node-resolve": "9.0.0",
        "@rollup/plugin-replace": "2.3.3",
        "@rollup/plugin-typescript": "3.1.1",
        "@types/body-parser": "1.19.0",
        "@types/debug": "4.1.5",
        "@types/express": "4.17.7",
        "@types/jest": "26.0.10",
        "@types/mongoose": "5.7.36",
        "@types/node": "14.6.0",
        "@types/randomstring": "1.1.6",
        "@types/supertest": "2.0.10",
        "@typescript-eslint/eslint-plugin": "3.9.1",
        "@typescript-eslint/parser": "3.9.1",
        "@vue/compiler-sfc": "3.0.0-rc.6",
        "@vue/test-utils": "1.0.4",
        "babel-core": "7.0.0-bridge.0",
        "casual": "1.6.2",
        "concurrently": "5.3.0",
        "eslint": "7.7.0",
        "eslint-plugin-vue": "6.2.2",
        "jest": "26.4.0",
        "mongodb-memory-server": "6.6.4",
        "nodemon": "2.0.4",
        "rollup": "2.26.4",
        "rollup-plugin-livereload": "1.3.0",
        "rollup-plugin-terser": "7.0.0",
        "rollup-plugin-vue": "5.1.6",
        "supertest": "4.0.2",
        "ts-jest": "26.2.0",
        "typescript": "3.9.7",
        "vue-eslint-parser": "7.1.0",
        "vue-jest": "3.0.6",
        "vue-template-compiler": "2.6.11"
    }
}
